{
  "buildCommand": "cd Frontend/my-react-app && npm install && npm run build",
  "outputDirectory": "Frontend/my-react-app/dist",
  "installCommand": "cd Frontend/my-react-app && npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 